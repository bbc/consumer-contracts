  "bugs": {
    "url": "https://github.com/bbc/consumer-contracts/issues"
  },
  "homepage": "https://github.com/bbc/consumer-contracts#readme",
  "dependencies": {
    "async": "^1.5.2",
    "chalk": "^1.0.0",
    "commander": "^2.8.1",
    "debug": "^2.2.0",
    "joi": "^6.10.1",
    "lodash": "^4.13.0",
    "request": "^2.55.0",
    "filehound": "^1.5.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.0",
    "istanbul": "^0.4.2",
    "jshint": "^2.8.0",
    "mocha": "^2.5.0",
    "nock": "^7.0.2",
    "sinon": "^1.17.3"
